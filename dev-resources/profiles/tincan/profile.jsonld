        "id": "http://id.tincanapi.com/activitytype/chat-channel",
        "inScheme": "http://registry.tincanapi.com",
        "type": "ActivityType",
        "definition": {
          "en": "A channel, room or conversation within an instant chat application such as Slack."
        },
        "prefLabel": {
          "en": "chat channel"
        }
    },

